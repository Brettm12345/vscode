{
  "eslintConfig": {
    "extends": "@brettm12345/eslint-config-node",
    "ignorePatterns": [
      "dist",
      ".log",
      "ts_compiled",
      "*.d.ts"
    ],
    "rules": {
      "unicorn/filename-case": [
        1,
        {
          "case": "kebabCase"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint *.ts ./**/*.ts",
    "build": "tsc"
  },
  "dependencies": {
    "@types/fs-extra": "^8.1.0",
    "@types/node": "^13.9.0",
    "fp-ts": "^2.5.3",
    "fs": "^0.0.1-security",
    "fs-extra": "^8.1.0",
    "path": "^0.12.7",
    "typescript": "^3.8.3",
    "vscode": "^1.1.36",
    "vscode-use-package": "^0.1.3"
  },
  "devDependencies": {
    "@brettm12345/eslint-config-node": "^1.0.6",
    "eslint": "^6.8.0",
    "eslint-plugin-prefer-arrow-functions": "^3.0.1",
    "prettier": "^1.19.1",
    "tsdx": "^0.12.3"
  }
}
