{
  "cSpell.enabled": false,
  "clairvoyant.excludeDirectories": ["node_modules", "dist", "ts_compiled"],
  "clairvoyant.excludeExtentions": ["yarn.lock", "settings.bkp.json", "log"],
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/dist/**": true,
    "**/ts_compiled/**": true
  },
  "files.exclude": {
    "dist": true,
    "ts_compiled": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/ts_compiled": true,
    "**/dist": true,
    "yarn.lock": true,
    "settings.bkp.json": true,
    "*.log": true
  }
}
