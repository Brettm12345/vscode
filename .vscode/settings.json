{
  "cSpell.enabled": false,
  "clairvoyant.excludeDirectories": [
    "node_modules",
    "dist",
    "ts_compiled"
  ],
  "clairvoyant.excludeExtentions": [
    "yarn.lock",
    "settings.bkp.json",
    "log"
  ],
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/ts_compiled": true,
    "**/dist": true,
    "yarn.lock": true,
    "settings.bkp.json": true,
    "*.log": true
  }
}