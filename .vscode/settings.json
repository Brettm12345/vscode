{
  "cSpell.enabled": false,
  "clairvoyant.excludeDirectories": ["node_modules", "dist", "ts_compiled"],
  "clairvoyant.excludeExtentions": ["yarn.lock", "settings.bkp.json", "log"],
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "dist": true,
    "ts_compiled": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/ts_compiled": true,
    "**/dist": true,
    "yarn.lock": true,
    "settings.bkp.json": true,
    "*.log": true
  },
  "workbench.colorCustomizations": {
    "[Moonlight II]": {
      "syntax.type": "#ffc777",
      // "constant": "#ff98a4",
      "syntax.function": "#82aaff",
      // "keyword": "#c099ff",
      // "number": "#ff966c",
      "syntax.operator": "#86e1fc",
      // "parameter": "#fca7ea",
      "syntax.directive": "#4fd6be",
      "syntax.string": "#c3e88d",
      "syntax.variable": "#ff98a4"
    }
  }
}
