{
    "init-script.path": "~/etc/vscode/init",
    "editor.accessibilitySupport": "off",
    "explorer.confirmDelete": false,
    "files.autoSave": "off",
    "search.exclude": {
        "**/.history": true,
        "**/bower_components": true,
        "**/node_modules": true
    },
    "terminal.integrated.shell.linux": "/run/current-system/sw/bin/zsh",
    "workbench.settings.openDefaultSettings": false,
    "editor.cursorBlinking": "smooth",
    "terminal.integrated.cursorStyle": "line",
    "workbench.editor.showTabs": true,
    "editor.autoClosingQuotes": "always",
    "explorer.confirmDragAndDrop": false,
    "files.exclude": {
        "**/.DS_Store": true,
        "**/.git": true,
        "**/.hg": true,
        "**/.history": true,
        "**/.svn": true,
        "**/CVS": true,
        "**/node_modules": true
    },
    "terminal.integrated.shellArgs.linux": [
        "-l"
    ],
    "workbench.settings.useSplitJSON": false,
    "editor.cursorSmoothCaretAnimation": false,
    "terminal.integrated.drawBoldTextInBrightColors": false,
    "workbench.list.automaticKeyboardNavigation": true,
    "editor.formatOnPaste": true,
    "files.trimTrailingWhitespace": true,
    "editor.cursorStyle": "line-thin",
    "terminal.integrated.experimentalRefreshOnResume": true,
    "workbench.list.horizontalScrolling": true,
    "editor.formatOnSave": true,
    "editor.cursorSurroundingLines": 10,
    "terminal.integrated.fontFamily": "monospace",
    "workbench.tree.indent": 8,
    "editor.fontLigatures": true,
    "terminal.integrated.fontSize": 10,
    "editor.fontWeight": "500",
    "editor.hover.delay": 150,
    "editor.insertSpaces": true,
    "editor.lineHeight": 23,
    "editor.lineNumbers": "relative",
    "editor.minimap.enabled": true,
    "editor.minimap.maxColumn": 400,
    "editor.minimap.renderCharacters": false,
    "editor.minimap.scale": 2,
    "editor.minimap.showSlider": "always",
    "editor.renderIndentGuides": true,
    "editor.renderLineHighlight": "all",
    "editor.renderWhitespace": "none",
    "editor.rulers": [
        80
    ],
    "editor.showFoldingControls": "mouseover",
    "editor.smoothScrolling": true,
    "editor.suggestFontSize": 12,
    "editor.suggestLineHeight": 22,
    "editor.suggestSelection": "first",
    "rust.cfg_test": false,
    "rust.all_features": false,
    "rust.clippy_preference": "on",
    "rust.unstable_features": true,
    "eslint.alwaysShowStatus": false,
    "eslint.packageManager": "yarn",
    "importCost.showCalculatingDecoration": true,
    "importCost.largePackageColor": "#ff5370",
    "importCost.mediumPackageColor": "#FF966C",
    "importCost.smallPackageColor": "#c3e88d",
    "importSorter.sortConfiguration.removeUnusedImports": false,
    "importSorter.importStringConfiguration.hasSemicolon": false,
    "importSorter.importStringConfiguration.tabSize": 2,
    "importSorter.sortConfiguration.importMembers.order": "lowercaseLast",
    "importSorter.sortConfiguration.importPaths.order": "lowercaseLast",
    "importSorter.generalConfiguration.sortOnBeforeSave": false,
    "importSorter.importStringConfiguration.maximumNumberOfImportExpressionsPerLine.count": 60,
    "importSorter.importStringConfiguration.maximumNumberOfImportExpressionsPerLine.type": "newLineEachExpressionAfterCountLimitExceptIfOnlyOne",
    "auto-close-tag.activationOnLanguage": [
        "xml",
        "php",
        "javascript",
        "javascriptreact",
        "typescriptreact",
        "plaintext",
        "markdown",
        "vue",
        "HTML (Eex)"
    ],
    "local-history.absolute": true,
    "local-history.path": "/home/brett/var/vscode/history"
}