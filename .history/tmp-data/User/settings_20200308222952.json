{
    "init-script.path": "~/etc/vscode/init",
    "editor.accessibilitySupport": "off",
    "editor.cursorBlinking": "smooth",
    "editor.autoClosingQuotes": "always",
    "editor.cursorSmoothCaretAnimation": false,
    "editor.formatOnPaste": true,
    "editor.cursorStyle": "line-thin",
    "editor.formatOnSave": true,
    "editor.cursorSurroundingLines": 10,
    "bracket-pair-colorizer-2.colors": [
        "#82AAFF",
        "#73daca",
        "#ff98a4",
        "#c3a2ff",
        "#ff757f",
        "#0ec3e3",
        "#f7a4ec",
        "#ffc777",
        "#C3E88D"
    ],
    "editor.formatOnType": true,
    "editor.fontLigatures": true,
    "bracket-pair-colorizer-2.scopeLineCSS": [
        "borderStyle : solid",
        "borderWidth : 0.5px",
        "borderColor : {color}",
        "opacity: 0.8"
    ],
    "editor.mouseWheelScrollSensitivity": 3,
    "editor.fontWeight": "500",
    "bracket-pair-colorizer-2.showBracketsInGutter": true,
    "editor.mouseWheelZoom": true,
    "editor.hover.delay": 150,
    "bracket-pair-colorizer-2.showBracketsInRuler": true,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.insertSpaces": true,
    "bracket-pair-colorizer-2.showVerticalScopeLine": true,
    "workbench.colorTheme": "Moonlight II",
    "editor.tabSize": 2,
    "editor.lineHeight": 23,
    "workbench.colorCustomizations": {
        "[Moonlight II Italic]": {
            "activityBar.background": "#161a2a"
        },
        "[Moonlight II]": {
            "activityBar.background": "#161a2a"
        }
    },
    "explorer.confirmDelete": false,
    "editor.lineNumbers": "relative",
    "explorer.confirmDragAndDrop": false,
    "editor.minimap.enabled": true,
    "files.autoSave": "off",
    "editor.minimap.maxColumn": 400,
    "files.exclude": {
        "**/.DS_Store": true,
        "**/.git": true,
        "**/.hg": true,
        "**/.history": true,
        "**/.svn": true,
        "**/CVS": true,
        "**/node_modules": true
    },
    "editor.minimap.renderCharacters": false,
    "files.trimTrailingWhitespace": true,
    "editor.minimap.scale": 2,
    "material-icon-theme.activeIconPack": "react",
    "editor.minimap.showSlider": "always",
    "material-icon-theme.folders.color": "#7c85b3",
    "editor.renderIndentGuides": true,
    "material-icon-theme.folders.theme": "specific",
    "editor.renderLineHighlight": "all",
    "prettifySymbolsMode.adjustCursorMovement": true,
    "material-icon-theme.languages.associations": {
        "dhall": "yaml"
    },
    "editor.renderWhitespace": "none",
    "prettifySymbolsMode.prettyCursor": "boxed",
    "material-icon-theme.files.associations": {
        ".envrc": "tune",
        "jest.teardown.ts": "jest"
    },
    "editor.rulers": [
        80
    ],
    "prettifySymbolsMode.revealOn": "cursor",
    "testExplorer.mergeSuites": true,
    "workbench.iconTheme": "material-icon-theme",
    "editor.showFoldingControls": "mouseover",
    "prettifySymbolsMode.substitutions": [
        {
            "language": "purescript",
            "substitutions": [
                {
                    "post": "\\b",
                    "pre": "\\b",
                    "pretty": "¬",
                    "ugly": "not\\s?"
                },
                {
                    "post": "\\b",
                    "pre": "\\b",
                    "pretty": "√",
                    "ugly": "sqrt\\s?"
                },
                {
                    "post": "[^=<>]|$",
                    "pre": "[^=<>]|^",
                    "pretty": "»",
                    "ugly": ">>"
                },
                {
                    "post": "[^=<>]|$",
                    "pre": "[^=<>]|^",
                    "pretty": "«",
                    "ugly": "<<"
                },
                {
                    "post": "\\b",
                    "pre": "\\b",
                    "pretty": "⟼",
                    "ugly": "return\\s?"
                },
                {
                    "post": "\\b",
                    "pre": "\\b",
                    "pretty": "¬",
                    "ugly": "not\\s?"
                },
                {
                    "post": "\\b",
                    "pre": "\\b",
                    "pretty": "∀",
                    "style": {
                        "color": "#c597f9"
                    },
                    "ugly": "forall"
                },
                {
                    "post": "\\s*(?:\\w|_).*?\\s*->",
                    "pretty": "λ ",
                    "style": {
                        "color": "#82aaff"
                    },
                    "ugly": "\\\\"
                }
            ]
        },
        {
            "language": "haskell",
            "substitutions": [
                {
                    "post": "\\b",
                    "pre": "\\b",
                    "pretty": "¬",
                    "ugly": "not\\s?"
                },
                {
                    "post": "\\b",
                    "pre": "\\b",
                    "pretty": "√",
                    "ugly": "sqrt\\s?"
                },
                {
                    "post": "\\b",
                    "pre": "\\b",
                    "pretty": "⊥",
                    "ugly": "undefined"
                },
                {
                    "post": "[^=<>]|$",
                    "pre": "[^=<>]|^",
                    "pretty": "»",
                    "ugly": ">>"
                },
                {
                    "post": "\\b",
                    "pre": "\\b",
                    "pretty": "⟼",
                    "ugly": "return"
                },
                {
                    "post": "[^=<>]|$",
                    "pre": "[^=<>]|^",
                    "pretty": "«",
                    "ugly": "<<"
                },
                {
                    "post": "\\b",
                    "pre": "\\b",
                    "pretty": "∀",
                    "style": {
                        "color": "#c597f9"
                    },
                    "ugly": "forall"
                },
                {
                    "post": "\\s",
                    "pre": "\\s",
                    "pretty": "∘",
                    "style": {
                        "color": "#86e1fc"
                    },
                    "ugly": "\\."
                },
                {
                    "post": "\\s*(?:\\w|_).*?\\s*->",
                    "pretty": "λ ",
                    "style": {
                        "color": "#82aaff"
                    },
                    "ugly": "\\\\"
                }
            ]
        }
    ],
    "testExplorer.addToEditorContextMenu": true,
    "editor.smoothScrolling": true,
    "testExplorer.showExpandButton": 0,
    "editor.suggestFontSize": 12,
    "testExplorer.showCollapseButton": false,
    "editor.suggestLineHeight": 22,
    "testExplorer.sort": "byLocation",
    "editor.suggestSelection": "first",
    "zentabs.maximumOpenedTabs": 20,
    "terminal.integrated.cursorStyle": "line",
    "zentabs.switchWithCurrentTab": true,
    "terminal.integrated.drawBoldTextInBrightColors": false,
    "terminal.integrated.experimentalRefreshOnResume": true,
    "terminal.integrated.fontFamily": "monospace",
    "terminal.integrated.fontSize": 10,
    "auto-close-tag.activationOnLanguage": [
        "xml",
        "php",
        "javascript",
        "javascriptreact",
        "typescriptreact",
        "plaintext",
        "markdown",
        "vue",
        "HTML (Eex)"
    ],
    "local-history.absolute": true,
    "local-history.path": "/home/brett/var/vscode/history"
}